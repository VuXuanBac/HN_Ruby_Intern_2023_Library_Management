<% content_for :head do %>
  <%= stylesheet_link_tag "admin/base", "data-turbo-track": "reload" %>
  <%= javascript_import_module_tag "admin/base" %>
<% end %>
<% content_for :body do %>
  <div class="container-scroller">
    <%= render "sidebar" %>
    <div class="container-fluid page-body-wrapper">
      <%= render "navbar" %>
      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
            <%= yield %>
          </div>
        </div>
        <%= render "footer" %>
      </div>
    </div>
  </div>
<% end %>
<%= render template: "admin/layouts/application" %>
