<% populate_publisher(publisher) => {
  name:, email:, address:, about:, publisher_path:, id:
}%>
<% null = Settings.display_for_nil %>
<tr id="<%= id %>">
  <td><%= link_to name, publisher_path, title: about %></td>
  <td><%= email || null %></td>
  <td><%= address || null %></td>
  <td>
    <%= render "admin/shared/form_button", item: publisher, name: t("publishers._name") %>
  </td>
</tr>
