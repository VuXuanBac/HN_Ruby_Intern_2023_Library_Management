<% provide :title, t("admin.title.index", name: t("accounts._name")) %>
<% content_for :table_header  do %>
  <tr>
    <th>#</th>
    <%= render_table_header t("accounts._name"), :username %>
    <%= render_table_header t("users.full_name"), :name %>
    <%= render_table_header t("users.phone"), :phone %>
    <%= render_table_header t("users.dob"), :dob %>
    <%= render_table_header t("users.join_about"), :join %>
    <%= render_table_header t("users.status") %>
    <th></th>
  </tr>
<% end %>
<% content_for :table_body, render(partial: "user", collection: @users) %>
<% content_for :table_empty, name: :users %>
<%= render "admin/shared/index", name: :users %>
